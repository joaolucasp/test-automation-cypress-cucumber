{
  "name": "cypress-cucumber-typescript",
  "version": "1.0.0",
  "description": "QA Test Automation Framework using Cypress, Cucumber and TypeScript",
  "main": "",
  "scripts": {
    "cypress:open": "cypress open",
    "tag:smoke": "cypress run --env tags='@smoke'",

    "lint:eslint": "eslint .",
    "lint": "npm run lint:eslint; npm run lint:tsc",
    "lint:fix": "npm run lint:eslint -- --fix",
    "format": "prettier --write \"cypress/**/*.ts\""
  },
  "keywords": [
    "BDD",
    "cucumber",
    "cypress",
    "qa",
    "testing",
    "typescript"
  ],
  "author": {
    "name": "Jo√£o Lucas Pereira e Sousa",
    "email": "lucaaspereeira@gmail.com",
    "url": "https://github.com/joaolucasp"
  },
  "license": "MIT",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@faker-js/faker": "^8.1.0",
    "@types/mocha": "^9.1.1",
    "@types/cypress": "^1.1.3",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "cypress": "^13.6.1",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-file-upload": "^5.0.8",
    "cypress-localstorage-commands": "^2.2.4",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-plugin-api": "^2.11.1",
    "cypress-promise": "^1.1.0",
    "cypress-wait-until": "^2.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsonc": "^2.11.2",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-standard": "^5.0.0",
    "faker-br": "^0.4.1",
    "lodash": "^4.17.21",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "^2.7.1",
    "ts-loader": "^9.5.1",
    "tsify": "^5.0.4",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": [
      "cypress/e2e/steps/**/*.ts"
    ],
    "html": {
      "enabled": true
    }
  }
}
